<form action="/login" method="POST">
<h4>Log in</h4>
<% locals?.errors?.forEach(error => { %>
  <li><%= error.message %></li>
<% }) %>
  <label>
    <input type="text" name="username" />
  </label>
  <label>
    <input type="password" name="password" />
  </label>
  <button type="submit">Log in</button>
</form>
